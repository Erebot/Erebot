msgstr  "%with-words:\n"
        "    0: =#0= seconde;\n"
        "    2: =#0= secondes;\n"
        "    60/60: <%%min<;\n"
        "    61/60: <%%min<, >%with-words>;\n"
        "    3600/60: <%%hr<;\n"
        "    3601/60: <%%hr<, >%with-words>;\n"
        "    86400/86400: <%%day<;\n"
        "    86401/86400: <%%day<, >%with-words>;\n"
        "    604800/604800: <%%week<;\n"
        "    604801/604800: <%%week<, >%with-words>;\n"
        "%%min:\n"
        "    1: 1 minute;\n"
        "    2: =#0= minutes;\n"
        "%%hr:\n"
        "    1: 1 heure;\n"
        "    2: =#0= heures;\n"
        "%%day:\n"
        "    1: 1 jour;\n"
        "    2: =#0= jours;\n"
        "%%week:\n"
        "    1: 1 semaine;\n"
        "    2: =#0= semaines;"
