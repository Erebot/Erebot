msgid   "\n"
        "<b>Usage:</b> !<var name=\"trigger\"/> &lt;<u>filter</u>&gt; &lt;"
        "<u>input</u>&gt;.\n"
        "Transforms the given &lt;<u>input</u>&gt; using the given &lt;"
        "<u>filter</u>&gt;.\n"
        "The following filters are available: <for from=\"filters\" item="
        "\"filter\">\n"
        "<b><var name=\"filter\"/></b></for>.\n"
msgstr  "\n"
        "<b>Usage:</b> !<var name=\"trigger\"/> &lt;<u>filter</u>&gt; &lt;"
        "<u>input</u>&gt;.\n"
        "Transforms the given &lt;<u>input</u>&gt; using the given &lt;"
        "<u>filter</u>&gt;.\n"
        "The following filters are available: <for from=\"filters\" item="
        "\"filter\">\n"
        "<b><var name=\"filter\"/></b></for>.\n"
#
msgid   "\n"
        "Provides the <b><var name=\"trigger\"/></b> command which transforms "
        "the given\n"
        "input using some PHP filter.\n"
msgstr  "\n"
        "Provides the <b><var name=\"trigger\"/></b> command which transforms "
        "the given\n"
        "input using some PHP filter.\n"
#
msgid   "Could not register Filter trigger"
msgstr  "Could not register Filter trigger"
#
msgid   "No such filter \"<var name=\"filter\"/>\" or filter blocked."
msgstr  "No such filter \"<var name=\"filter\"/>\" or filter blocked."
#
msgid   "Usage: <b><var name=\"cmd\"/> &lt;filter&gt; &lt;text&gt;</b>. "
        "Available filters: <for from=\"filters\" item=\"filter\"><var name="
        "\"filter\"/></for>."
msgstr  "Usage: <b><var name=\"cmd\"/> &lt;filter&gt; &lt;text&gt;</b>. "
        "Available filters: <for from=\"filters\" item=\"filter\"><var name="
        "\"filter\"/></for>."
