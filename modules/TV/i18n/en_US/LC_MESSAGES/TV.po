msgid   "\n"
        "<b>Usage:</b> !<var name='trigger'/> [<u>time</u>] [<u>channels</"
        "u>].\n"
        "Returns TV schedules for the given channels at the given time.\n"
        "[<u>time</u>] can be expressed using either 12h or 24h notation.\n"
        "[<u>channels</u>] can be a single channel name, a list of channels\n"
        "(separated by commas) or one of the pre-defined groups of channels.\n"
msgstr  "\n"
        "<b>Usage:</b> !<var name='trigger'/> [<u>time</u>] [<u>channels</"
        "u>].\n"
        "Returns TV schedules for the given channels at the given time.\n"
        "[<u>time</u>] can be expressed using either 12h or 24h notation.\n"
        "[<u>channels</u>] can be a single channel name, a list of channels\n"
        "(separated by commas) or one of the pre-defined groups of channels.\n"
#
msgid   "\n"
        "Provides the <b><var name=\"trigger\"/></b> command which retrieves\n"
        "information about TV schedules off the internet.\n"
msgstr  "\n"
        "Provides the <b><var name=\"trigger\"/></b> command which retrieves\n"
        "information about TV schedules off the internet.\n"
#
msgid   "Could not register TV trigger"
msgstr  "Could not register TV trigger"
#
msgid   "If none is given, the default group (<b><var name='default'/></b>) is "
        "used. The following groups are available: <for from='groups' "
        "key='group' item='dummy'><b><var name='group'/></b></for>."
msgstr  "If none is given, the default group (<b><var name='default'/></b>) is "
        "used. The following groups are available: <for from='groups' "
        "key='group' item='dummy'><b><var name='group'/></b></for>."
#
msgid   "No channel given and no default."
msgstr  "No channel given and no default."
#
msgid   "No such channel(s)"
msgstr  "No such channel(s)"
#
msgid   "TV programs for <u><var name=\"date\"/></u>: <for from=\"programs\" "
        "key=\"channel\" item=\"timetable\" separator=\" - \"><b><var name="
        "\"channel\"/></b>: <var name=\"timetable\"/></for>"
msgstr  "TV programs for <u><var name=\"date\"/></u>: <for from=\"programs\" "
        "key=\"channel\" item=\"timetable\" separator=\" - \"><b><var name="
        "\"channel\"/></b>: <var name=\"timetable\"/></for>"
