msgid   "No such number or number already used"
msgstr  "Nombre inexistant ou déjà utilisé"
#
# File: Countdown.php, line: 141
msgid   "Non integral division"
msgstr  "Division non-entière"
#
# File: Countdown.php, line: 189
msgid   "Not bad <b><var name=\"nick\"/></b>, you actually got <b><var name="
        "\"result\"/></b>, but this is not the best formula... Try again ;)"
msgstr  "Pas mal <b><var name=\"nick\"/></b>, vous avez obtenu <b><var name="
        "\"result\"/></b>, mais ce n'est pas la meilleur formule... Réessayez ;)"
#
# File: Countdown.php, line: 74
msgid   "So far, <b><var name=\"nick\"/></b> has achieved <b><var name=\"result"
        "\"/></b> using this formula: <b><var name=\"formula\"/></b>"
msgstr  "Pour le moment <b><var name=\"nick\"/></b> a obtenu <b><var name=\"result"
        "\"/></b> avec cette formule : <b><var name=\"formula\"/></b>"
#
# File: Countdown.php, line: 145
msgid   "Syntax error"
msgstr  "Erreur de syntaxe"
#
