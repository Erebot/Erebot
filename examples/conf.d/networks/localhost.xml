<network
    xmlns="http://www.erebot.net/xmlns/erebot"
    name="localhost"
>
    <servers>
        <server url="irc://localhost:6667/"/>
    </servers>

    <channels>
        <channel name="#Erebot">
            <modules>
                <module name="Erebot_Module_AutoJoin" />
            </modules>
        </channel>
    </channels>
</network>

